                              <HBox alignment="CENTER" prefHeight="140.0" prefWidth="450.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#PaymentBtn" text="결제하기">
                                       <HBox.margin>
                                          <Insets top="30.0" />
                                       </HBox.margin>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
